{
  "version": 2,
  "buildCommand": "yarn vercel-build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "framework": null,
  "ignoreCommand": "git diff HEAD^ HEAD --quiet -- backend/ package.json yarn.lock",
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
